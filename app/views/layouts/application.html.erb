<!DOCTYPE html>
<html>
  <head>
    <title>Geocode</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyABM0IzG6lAnCGjQPiTFr6uOyR0mStgOnE&libraries=places&callback=initAutocomplete"
         async defer></script>

  </head>

  <body>
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <%= render '/notifications' %>

          <%= yield %>
        </div>
      </div>
    </div>
  </body>
</html>
